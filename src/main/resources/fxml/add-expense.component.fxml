<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>


<HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="financial.assistant.controllers.AddExpenseComponentController">
   <children>
      <TextField fx:id="expenseName" promptText="Expense Name" HBox.hgrow="ALWAYS" />
      <TextField fx:id="expenseCost" promptText="Expense Cost" HBox.hgrow="ALWAYS" />
      <DatePicker fx:id="expenseDueDate" promptText="Due Date" HBox.hgrow="ALWAYS" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
